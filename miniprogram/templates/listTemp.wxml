<template name="listTemp">
  <view class="list" wx:for="{{gameLists}}" wx:key="index">
    <image src="/icon/{{item.img}}"></image>
    <view class="list-font">
      <view class="title">{{item.title}}</view>
      <text class="follow">关注</text>
      <text class="try-play">试玩</text>
    </view>
    <button type="mini" class="list-btn">下载</button>
  </view>
</template>